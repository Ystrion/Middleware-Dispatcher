{
  "name": "ystrion/middleware-dispatcher",
  "description": "Two middleware dispatchers (PSR-15).",
  "version": "1.0.0",
  "type": "library",
  "homepage": "https://github.com/Ystrion/Middleware-Dispatcher",
  "readme": "README.MD",
  "license": "MIT",
  "authors": [
    {
      "name": "LÃ©o DEVILLE",
      "email": "public-github@deville.dev",
      "homepage": "https://deville.dev/",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "public-github@deville.dev",
    "issues": "https://github.com/Ystrion/Middleware-Dispatcher/issues",
    "forum": "https://github.com/Ystrion/Middleware-Dispatcher/discussions",
    "wiki": "https://github.com/Ystrion/Middleware-Dispatcher/wiki",
    "source": "https://github.com/Ystrion/Middleware-Dispatcher",
    "docs": "https://github.com/Ystrion/Middleware-Dispatcher"
  },
  "require": {
    "php": "^8.2",
    "psr/container": "^2.0",
    "psr/http-message": "^2.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "nyholm/psr7": "^1.8",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-deprecation-rules": "^1.1",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpstan/phpstan-strict-rules": "^1.5",
    "phpunit/phpunit": "^10.2",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "Ystrion\\MiddlewareDispatcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ystrion\\MiddlewareDispatcher\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "sort-packages": true
  },
  "scripts": {
    "phpcs": "phpcs",
    "phpstan": "phpstan --memory-limit=-1",
    "phpunit": "phpunit",
    "test": ["@phpcs", "@phpstan", "@phpunit"]
  }
}
